[Unit]
Description=Starting the FreeDi service
After=moonraker.service

[Service]
Type=simple
User=mks
WorkingDirectory=~/FreeDi/FreeDiLCD
ExecStartPre=/bin/sleep 10
ExecStart=/bin/sh -c '~/klippy-env/bin/python3 ~/FreeDi/FreeDiLCD/start.py  > /tmp/FreeDiLCD.log 2>&1'
Restart=always
RestartSec=1

[Install]
WantedBy=multi-user.target
