[Unit]
Description=Starting the FreeDiLCD service
After=moonraker.service

[Service]
Type=simple
User=mks
WorkingDirectory=/home/mks/FreeDi
ExecStartPre=/bin/sleep 10
ExecStart=/bin/sh -c '/usr/bin/env python3 /home/mks/FreeDi/FreeDiLCD/start.py  > /tmp/FreeDiLCD.log 2>&1'
Restart=always
RestartSec=1

[Install]
WantedBy=multi-user.target
